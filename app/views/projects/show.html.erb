<p><%= @project.name %></p>
<p>Material: <%= @project.material %></p>
<p>Challenge Theme: <%= @project.challenge.theme %></p>
<p>Number of Contestants: <%= @project.number_of_contestants %></p>
<% unless @project.average_contestant_experience.nan? %>
  <p>Average Contestant Experience: <%= @project.average_contestant_experience.round(2) %></p>
<% end %>
<br>
<%= form_tag("/projects/#{@project.id}", method: :patch) do  %>
  <%= label_tag :contestant_id %><br>
  <%= text_field_tag :contestant_id %><br><br>
  <%= submit_tag "Add Contestant To Project" %>
<% end %>
